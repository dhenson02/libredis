{
    "name": "libredis",
    "version": "1.0.0",
    "description": "Node API to connect and interface with Redis",
    "main": "build/index.js",
    "scripts": {
        "start": "npm run tsc && npm run app",
        "start-dev": "npm-run-all --parallel tsc-dev app ",
        "app": "node build/index.js",
        "tsc": "tsc",
        "tsc-dev": "tsc --watch",
        "test": "mocha ./**/*.test.js",
        "lint:test": "eslint -c .eslintrc.js '**/*.{js,ts,json}'",
        "lint:fix": "eslint -c .eslintrc.js --fix '**/*.{js,ts,json}'"
    },
    "keywords": [
        "redis",
        "node"
    ],
    "author": "Deryck Henson (@dhenson02)",
    "license": "MIT",
    "devDependencies": {
        "chai": "^4.3.4",
        "eslint": "^8.5.0",
        "mocha": "^9.1.3",
        "npm-run-all": "^4.1.5",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "@types/chai": "^4.3.0",
        "@types/eslint": "^8.2.1",
        "@types/mocha": "^9.0.0",
        "@types/node": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "dataloader": "^2.0.0",
        "dotenv": "^10.0.0",
        "pino": "^7.5.1"
    }
}
